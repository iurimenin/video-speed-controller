{
    "manifest_version": 3,
    "name": "Video Speed Controller",
    "version": "1.0",
    "description": "Controle a velocidade de vídeos em qualquer página.",
    "permissions": ["scripting", "activeTab"],
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"],
        "css": ["style.css"]
      }
    ],
    "icons": {
      "48": "icon.png"
    }
  }
  