{
  "manifest_version": 3,
  "name": "Video Speed Controller",
  "version": "1.1",
  "description": "Controle a velocidade de vídeos ao clicar na extensão.",
  "permissions": ["scripting", "activeTab"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "",
    "default_icon": {
      "48": "icon.png"
    }
  },
  "icons": {
    "48": "icon.png"
  }
}
